er-dom\";\nimport List from \"../../components/List/List\";\nimport useFetch from \"../../hooks/useFetch\";\nimport \"./Products.scss\";\n\nconst Products = () => {\n  const catId = parseInt(useParams().id);\n  const [maxPrice, setMaxPrice] = useState(1000);\n  const [sort, setSort] = useState(null);\n  const [selectedSubCats, setSelectedSubCats] = useState([]);\n\n  const { data, loading, error } = useFetch(\n    `/sub-categories?[filters][categories][id][$eq]=${catId}`\n  );\n\n  const handleChange = (e) => {\n    const value = e.target.value;\n    const isChecked = e.target.checked;\n\n    setSelectedSubCats(\n      isChecked\n        ? [...selectedSubCats, value]\n        : selectedSubCats.filter((item) => item !== value)\n    );\n  };\n\n  return (\n    <div className=\"products\">\n      <div className=\"left\">\n        <div className=\"filterItem\">\n          <h2>Categorias</h2>\n          {data?.map((item) => (\n            <div className=\"inputItem\" key={item.id}>\n              <input\n                type=\"checkbox\"\n                id={item.id}\n                value={item.id}\n                onChange={handleChange}\n              />\n              <label htmlFor={item.id}>{item.attributes.title}</label>\n            </div>\n          ))}\n        </div>\n        <div className=\"filterItem\">\n          <h2>Filtrar por preço</h2>\n          <div className=\"inputItem\">\n            <span>0</span>\n            <input\n              type=\"range\"\n              min={0}\n              max={1000}\n              onChange={(e) => setMaxPrice(e.target.value)}\n            />\n            <span>{maxPrice}</span>\n          </div>\n        </div>\n        <div className=\"filterItem\">\n          <h2>Ordenar</h2>\n          <div className=\"inputItem\">\n            <input\n              type=\"radio\"\n              id=\"asc\"\n              value=\"asc\"\n              name=\"price\"\n              onChange={(e) => setSort(\"asc\")}\n            />\n            <label htmlFor=\"asc\">Menor Preço</label>\n          </div>\n          <div className=\"inputItem\">\n            <input\n              type=\"radio\"\n              id=\"desc\"\n              value=\"desc\"\n              name=\"price\"\n              onChange={(e) => setSort(\"desc\")}\n            />\n            <label htmlFor=\"desc\">Maior Preço</label>\n          </div>\n        </div>\n      </div>\n      <div className=\"right\">\n        <img\n          className=\"catImg\"\n          src=\"https://images.pexels.com/photos/1074535/pexels-photo-1074535.jpeg?auto=compress&cs=tinysrgb&w=1600\"\n          alt=\"\"\n        />\n        <List\n          catId={catId}\n          maxPrice={maxPrice}\n          sort={sort}\n          subCats={selectedSubCats}\n        />\n      </div>\n    </div>\n  );\n};\n\nexport default Products;\n","/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/pages/Product/Product.jsx",["100"],[],"import React from \"react\";\nimport { useState } from \"react\";\nimport \"./Product.scss\";\nimport AddShoppingCartIcon from \"@mui/icons-material/AddShoppingCart\";\nimport FavoriteBorderIcon from \"@mui/icons-material/FavoriteBorder\";\nimport BalanceIcon from \"@mui/icons-material/Balance\";\nimport useFetch from \"../../hooks/useFetch\";\nimport { useParams } from \"react-router-dom\";\nimport { useDispatch } from \"react-redux\";\nimport { addToCart } from \"../../redux/cartReducer\";\n\nconst Product = () => {\n  const id = useParams().id;\n  const [selectedImg, setSelectedImg] = useState(\"img\");\n  const [quantity, setQuantity] = useState(1);\n\n  const dispatch = useDispatch();\n  const { data, loading, error } = useFetch(`/products/${id}?populate=*`);\n\n  return (\n    <div className=\"product\">\n      {loading ? (\n        \"loading\"\n      ) : (\n        <>\n          <div className=\"left\">\n            <div className=\"images\">\n              <img\n                src={\n                  process.env.REACT_APP_UPLOAD_URL +\n                  data?.attributes?.img?.data?.attributes?.url\n                }\n                alt=\"\"\n                onClick={(e) => setSelectedImg(\"img\")}\n              />\n              <img\n                src={\n                  process.env.REACT_APP_UPLOAD_URL +\n                  data?.attributes?.img2?.data?.attributes?.url\n                }\n                alt=\"\"\n                onClick={(e) => setSelectedImg(\"img2\")}\n              />\n            </div>\n            <div className=\"mainImg\">\n              <img\n                src={\n                  process.env.REACT_APP_UPLOAD_URL +\n                  data?.attributes[selectedImg]?.data?.attributes?.url\n                }\n                alt=\"\"\n              />\n            </div>\n          </div>\n          <div className=\"right\">\n            <h1>{data?.attributes?.title}</h1>\n            <span className=\"price\">${data?.attributes?.price}</span>\n            <p>{data?.attributes?.desc}</p>\n            <div className=\"quantity\">\n              <button\n                onClick={() =>\n                  setQuantity((prev) => (prev === 1 ? 1 : prev - 1))\n                }\n              >\n                -\n              </button>\n              {quantity}\n              <button onClick={() => setQuantity((prev) => prev + 1)}>+</button>\n            </div>\n            <button\n              className=\"add\"\n              onClick={() =>\n                dispatch(\n                  addToCart({\n                    id: data.id,\n                    title: data.attributes.title,\n                    desc: data.attributes.desc,\n                    price: data.attributes.price,\n                    img: data.attributes.img.data.attributes.url,\n                    quantity,\n                  })\n                )\n              }\n            >\n              <AddShoppingCartIcon /> ADICIONAR AO CARRINHO\n            </button>\n            <div className=\"links\">\n              <div className=\"item\">\n                <FavoriteBorderIcon /> LISTA DE DESEJOS\n              </div>\n              <div className=\"item\">\n                <BalanceIcon /> COMPARAR\n              </div>\n            </div>\n            <div className=\"info\">\n              <span>Vendor: Polo</span>\n              <span>Product Type: T-Shirt</span>\n              <span>Tag: T-Shirt, Women, Top</span>\n            </div>\n            <hr />\n            <div className=\"info\">\n              <span>DESCRIÇÃO</span>\n              <hr />\n              <span>INFORMAÇÃO ADICIONAL</span>\n              <hr />\n              <span>FAQ</span>\n            </div>\n          </div>\n        </>\n      )}\n    </div>\n  );\n};\n\nexport default Product;\n","/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/Navbar/Navbar.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/Footer/Footer.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/Slider/Slider.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/FeaturedProducts/FeaturedProducts.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/Card/Card.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/Categories/Categories.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/Contact/Contact.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/List/List.jsx",["101"],[],"import React from \"react\";\nimport \"./List.scss\";\nimport Card from \"../Card/Card\";\nimport useFetch from \"../../hooks/useFetch\";\n\nconst List = ({ subCats, maxPrice, sort, catId }) => {\n  const { data, loading, error } = useFetch(\n    `/products?populate=*&[filters][categories][id]=${catId}${subCats.map(\n      (item) => `&[filters][sub_categories][id][$eq]=${item}`\n    )}&[filters][price][$lte]=${maxPrice}&sort=price:${sort}`\n  );\n\n  return (\n    <div className=\"list\">\n      {loading\n        ? \"loading\"\n        : data?.map((item) => <Card item={item} key={item.id} />)}\n    </div>\n  );\n};\n\nexport default List;\n","/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/components/Cart/Cart.jsx",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/makeRequest.js",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/hooks/useFetch.js",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/redux/cartReducer.js",[],[],"/Users/eduardovinagre/Documents/Mentoria/Dev/react-ecommerce/client/src/redux/store.js",[],[],{"ruleId":"102","severity":1,"message":"103","line":1,"column":10,"nodeType":"104","messageId":"105","endLine":1,"endColumn":18},{"ruleId":"102","severity":1,"message":"106","line":14,"column":17,"nodeType":"104","messageId":"105","endLine":14,"endColumn":24},{"ruleId":"102","severity":1,"message":"107","line":14,"column":26,"nodeType":"104","messageId":"105","endLine":14,"endColumn":31},{"ruleId":"102","severity":1,"message":"107","line":18,"column":26,"nodeType":"104","messageId":"105","endLine":18,"endColumn":31},{"ruleId":"102","severity":1,"message":"107","line":7,"column":26,"nodeType":"104","messageId":"105","endLine":7,"endColumn":31},"no-unused-vars","'Children' is defined but never used.","Identifier","unusedVar","'loading' is assigned a value but never used.","'error' is assigned a value but never used."]